#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(int argc, char *argv[]) {
    int num;
    while(scanf("%d",&num)!=EOF){
        int isPrime = 1; 
        for (int i = 2; i <= sqrt(num); i++) { 
            if (num % i == 0) { 
            isPrime = 0;
            break;
         }
        }
        if (isPrime) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }
	return 0;
}
