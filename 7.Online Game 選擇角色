#include <iostream>
using namespace std;

int main() {
    int number;
    while (cin >> number) {
        if (number == 1) {
            cout << "Person\n";
        } else if (number == 2) {
            cout << "Fairy\n";
        } else {
            cout << "Dwarf\n";
        }
    }
    return 0;
}
