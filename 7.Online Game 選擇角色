#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    int number;
    while(scanf("%d",&number)!=EOF){
        if(number==1){
            printf("Person\n");
        }else if(number==2){
            printf("Fairy\n");
        }else{
            printf("Dwarf\n");
        }
    }
	return 0;
}
