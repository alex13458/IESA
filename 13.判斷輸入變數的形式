#include <iostream>
#include <string>
#include <cctype>

using namespace std;
int main() {
    string input;
    while (getline(cin, input)) {
        // 移除開頭與結尾空白
        input.erase(0, input.find_first_not_of(" \t"));
        input.erase(input.find_last_not_of(" \t") + 1);

        int len = input.length();

        // 判斷 char
        if (len == 1 && isprint(input[0])) {
            cout << "char" << endl;
            continue;
        }

        // 判斷 float
        int dot_count = 0;
        bool is_valid_float = true;
        for (int i = 0; i < len; ++i) {
            if (input[i] == '.') {
                dot_count++;
            } else if (!isdigit(input[i]) && input[i] != '-') {
                is_valid_float = false;
                break;
            }
        }

        if (is_valid_float && dot_count == 1) {
            cout << "float" << endl;
            continue;
        }

        // 判斷 int
        bool is_valid_int = true;
        for (int i = 0; i < len; ++i) {
            if (!isdigit(input[i]) && input[i] != '-') {
                is_valid_int = false;
                break;
            }
        }

        if (is_valid_int) {
            cout << "int" << endl;
        } else {
            cout << "string" << endl;
        }
    }

    return 0;
}
