#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main(int argc, char *argv[]) {
    char input[100];  
    while (fgets(input, sizeof(input), stdin) != NULL) {
        input[strcspn(input, "\n")] = '\0';
        
        int len = strlen(input);
        if (len == 1 && isprint(input[0])) {
            printf("char\n");
            continue;
        }
        int is_float = 0;
        for (int i = 0; i < len; i++) {
            if (input[i] == '.') {
                is_float = 1; 
            } else if (!isdigit(input[i]) && input[i] != '-' && input[i] != '.') {//檢查是否數字
                is_float = 0; 
            }
        }
        if (is_float) {
            printf("float\n");
        }
        else {
            int is_int = 1;
            for (int i = 0; i < len; i++) {
                if (!isdigit(input[i]) && input[i] != '-' && input[i] != ' ') {
                    is_int = 0; 
                    break;
                }
            }
            
            if (is_int) {
                printf("int\n");
            }
            else {
                printf("string\n");
            }
        }
    }
	return 0;
}
