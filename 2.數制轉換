#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main() {
    int base, number;
    const string digits = "0123456789ABCDEF";

    while (cin >> base >> number) {
        if (number == 0) {
            cout << "0" << endl;
            continue;
        }
        string result;
        while (number > 0) { // 將數字轉換為指定進制
            result += digits[number % base];
            number /= base;
        }     
        reverse(result.begin(), result.end()); // 反转字符串
        cout << result << endl;
    }

    return 0;
}
