#include <iostream>
using namespace std;
int main(int argc, char *argv[]) {
    int w,h;
    double BMI;
    while(cin>>w>>h){
        double height= h / 100.0;
        BMI=w / (height*height);
        cout << fixed;
        cout.precision(2); //取到小數點以下第一位
        cout << BMI << endl;
    }
	return 0;
}
