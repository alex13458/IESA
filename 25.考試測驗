#include <stdio.h>
#include <stdlib.h>
int main(void)
{   
    int N;
    int listening, reading, speaking;
    scanf("%d", &N); 
    for (int i = 0; i < N; i++) {  
        scanf("%d%d%d", &listening, &reading, &speaking);
        if (listening >= 60 && reading >= 60 && speaking >= 60) {
            printf("P\n");  
        } 
        else if (listening + reading + speaking >= 220) {
            printf("P\n");  
        } else if (listening < 60 && reading < 60 && speaking >= 80){
            printf("M\n");
        }else if(reading < 60 && speaking < 60 && listening >= 80){
            printf("M\n");
        }else if(speaking < 60 && listening < 60 && reading >= 80){
            printf("M\n");
        } else if(listening >= 60 && reading >= 60 ) {
            printf("M\n"); 
        }else if(listening >= 60 && speaking >= 60 ) {
            printf("M\n"); 
        }else if(speaking >= 60 && reading >= 60 ) {
            printf("M\n"); 
        }else {
            printf("F\n");  
        }
	}
	return 0;
}
