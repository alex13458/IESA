#include <iostream>
using namespace std;

int main(void) {
    int N;
    cin >> N;
    for (int i = 0; i < N; i++) {
        int listening, reading, speaking;
        cin >> listening >> reading >> speaking;
        //三科60以上或一個科未滿 60，但總分超過 220 
        if ((listening >= 60 && reading >= 60 && speaking >= 60) || (listening + reading + speaking >= 220)) {
            cout << "P" << endl;
        } else if (
             // 兩科低於60，一科 >=80
            ((listening < 60 && reading < 60 && speaking >= 80) ||
             (reading < 60 && speaking < 60 && listening >= 80) ||
             (speaking < 60 && listening < 60 && reading >= 80)) ||
             // 任兩科 >=60
            ((listening >= 60 && reading >= 60) ||
             (listening >= 60 && speaking >= 60) ||
             (reading >= 60 && speaking >= 60))
        ) {
            cout << "M" << endl;
        } else {
            cout << "F" << endl;
        }
    }
    return 0;
}
