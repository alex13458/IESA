#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main(int argc, char *argv[]) {
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        string goo;
        cin >> goo;
        reverse(goo.begin(), goo.end());// 反轉輸入字串
        cout << goo << "\n";
    }
    return 0;
}
