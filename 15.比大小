#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    int a,b,c;
    while(scanf("%d%d%d",&a,&b,&c)!=EOF){
        int max = a;
        if (b > max) {
            max = b;
        }
        if (c > max) {
            max = c;
        }
        printf("%d\n", max);
    }
	return 0;
}
