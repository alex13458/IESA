#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
   int score;
    while (cin >> score) {
        if (score < 0 || score > 100) {
            cout << "error\n";
        } else if (score >= 90) {//90～100分之間為A
            cout << "A\n";
        } else if (score >= 80) {//80～89分為B
            cout << "B\n";
        } else if (score >= 70) {//70～79分為C
            cout << "C\n";
        } else if (score >= 60) {//60～69分為D
            cout << "D\n";
        } else {                 //未滿60為E
            cout << "E\n";
        }
    }
    return 0;
}
