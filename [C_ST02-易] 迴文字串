#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc, char *argv[]) {
    int n;
    scanf("%d", &n);  
    for (int i = 0; i < n; i++) {
        char goo[257];  
        scanf("%s", goo);  

        int length = strlen(goo);  
        for (int j = length - 1; j >= 0; j--) {
            printf("%c", goo[j]);
        }
        printf("\n"); 
    }
	return 0;
}
