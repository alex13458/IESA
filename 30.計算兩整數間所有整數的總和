#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
    int n;
    cin >>n;
    for (int i = 0; i < n; i++) {
        int x, y,ans=0;
        cin >> x >> y;
        if (x > y) {
            int swap = x;
            x = y;
            y = swap;
        }
        for (int j = x; j <= y; j++) {
            ans =ans + j;
        }
        cout << ans << endl;
    }
	return 0;
}
