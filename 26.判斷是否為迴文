#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    int num, reversed_num = 0, original_num, change;
    while(scanf("%d", &num)!=EOF){
      original_num = num;
      while (num != 0) {
        change = num % 10;  
        reversed_num = reversed_num * 10 + change;  
        num =num/10;  
      }
    if (original_num == reversed_num) {
        printf("YES\n"); 
      } else {
        printf("NO\n");  
      }
  }
	return 0;
}
