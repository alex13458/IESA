#include <iostream>
#include <cmath>
using namespace std;

int main(int argc, char *argv[]) {
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        double w;
        cin >> w;
        double ans = w * w;
        ans = round(ans * 10) / 10;  
        cout << fixed;
        cout.precision(1); //取到小數點以下第一位
        cout << ans<< endl;
    }
    return 0;
}
