#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
     int a, b, c;
    cin >> a >> b >> c;
    int swap;
    if (a < b) {
        swap = a;
        a = b;
        b = swap;
    }
    if (a < c) {
        swap = a;
        a = c;
        c = swap;
    }
    if (b < c) {
        swap = b;
        b = c;
        c = swap;
    }
    cout << a << ">" << b << ">" << c << "\n";
    return 0;
}
