#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
    int N;
    cin >>N;
    int total = 0;
    for (int i = 1; i <= N; i++) {
        if ((i % 2 == 0 && i % 3 == 0) && i % 12 != 0) {
            total =total + i;
        }
    }
        cout << total << endl;
	return 0;
}
