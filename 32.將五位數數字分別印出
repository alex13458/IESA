#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
     int num;
    while (cin >> num) {
        int ans[5];
        for (int i = 4; i >= 0; i--) {
            ans[i] = num % 10;
            num /= 10;
        }
        for (int i = 0; i < 5; i++) {
            cout << ans[i];
            if (i < 4) {
                cout << "   "; 
            }    
        }
        cout << endl;
    }
    return 0;
}
